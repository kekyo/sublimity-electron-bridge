{
  "name": "sublimity-electron-bridge-cli",
  "version": "0.11.1",
  "description": "Electron bridge generator for automatic IPC setup",
  "repository": {
    "type": "git",
    "url": "https://github.com/kekyo/sublimity-electron-bridge.git"
  },
  "homepage": "https://github.com/kekyo/sublimity-electron-bridge#readme",
  "keywords": [
    "electron",
    "ipc",
    "bridge",
    "automatic",
    "generator",
    "cli"
  ],
  "main": "dist/index.js",
  "bin": {
    "seb": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rv --npm . && npm i && vite build",
    "dev": "rv --npm . && npm i && vite build --watch",
    "test": "rv --npm . && npm i && vitest run",
    "test:ui": "rv --npm . && npm i && vitest --ui",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/node": "^24.0.10",
    "@vitest/ui": "^3.2.4",
    "typescript": "^5.8.3",
    "vite": "^7.0.2",
    "vitest": "^3.2.4"
  }
}