{
  "name": "sublimity-electron-bridge-cli",
  "description": "Electron bridge generator for automatic IPC setup (CLI)",
  "keywords": [
    "electron",
    "ipc",
    "bridge",
    "automatic",
    "generator",
    "cli"
  ],
  "main": "dist/index.js",
  "bin": {
    "seb": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "test": "npm run build && vitest run",
    "pack": "npm run build && screw-up pack --pack-destination ../artifacts"
  },
  "peerDependencies": {
    "commander": ">=11.1.0",
    "sublimity-electron-bridge-core": "*",
    "typescript": ">=5.0.0"
  },
  "devDependencies": {
    "@types/node": ">=24.0.10",
    "@vitest/ui": ">=3.2.4",
    "screw-up": ">=0.12.0",
    "typescript": ">=5.8.3",
    "vite": ">=7.0.2",
    "vitest": ">=3.2.4"
  }
}