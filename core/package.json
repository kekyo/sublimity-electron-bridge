{
  "name": "sublimity-electron-bridge-core",
  "version": "0.0.1",
  "description": "Core logic for Electron bridge generation",
  "keywords": [
    "electron",
    "typescript",
    "bridge"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "test": "npm run build && vitest run",
    "pack": "npm run build && screw-up pack --pack-destination ../artifacts"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0",
    "sublimity-rpc": ">=1.4.0"
  },
  "devDependencies": {
    "@types/node": ">=24.0.10",
    "@vitest/ui": ">=3.2.4",
    "screw-up": ">=1.0.0",
    "vite": ">=7.0.2",
    "vite-plugin-dts": ">=4.3.0",
    "vitest": ">=3.2.4",
    "dayjs": ">=1.11.13"
  }
}
