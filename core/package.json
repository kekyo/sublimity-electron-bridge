{
  "name": "sublimity-electron-bridge-core",
  "version": "0.15.4",
  "description": "Core logic for Electron bridge generation",
  "keywords": [
    "electron",
    "typescript",
    "ast",
    "bridge"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rv --npm . && npm i && tsc --noEmit && vite build",
    "test": "rv --npm . && npm i && tsc --noEmit && vitest run"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0",
    "sublimity-rpc": ">=0.2.1"
  },
  "devDependencies": {
    "@types/node": ">=24.0.10",
    "@vitest/ui": ">=3.2.4",
    "screw-up": ">=0.8.1",
    "vite": ">=7.0.2",
    "vite-plugin-dts": ">=4.3.0",
    "vitest": ">=3.2.4",
    "dayjs": ">=1.11.13"
  }
}