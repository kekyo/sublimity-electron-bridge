{
  "name": "sublimity-electron-bridge-vite",
  "version": "0.2.12",
  "description": "Electron bridge generator for automatic IPC setup",
  "repository": {
    "type": "git",
    "url": "https://github.com/kekyo/sublimity-electron-bridge.git"
  },
  "homepage": "https://github.com/kekyo/sublimity-electron-bridge#readme",
  "keywords": [
    "electron",
    "ipc",
    "bridge",
    "automatic",
    "generator",
    "vite",
    "plugin"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rv --npm . && npm i && vite build",
    "dev": "rv --npm . && npm i && vite build --watch",
    "test": "rv --npm . && npm i && vitest run",
    "test:ui": "rv --npm . && npm i && vitest --ui",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "glob": "^11.0.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.2",
    "vite-plugin-dts": "^4.5.4",
    "sublimity-electron-bridge-core": "file:../core"
  },
  "devDependencies": {
    "@types/node": "^24.0.10",
    "@vitest/ui": "^3.2.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}