{
  "name": "sublimity-electron-bridge-vite",
  "version": "0.0.1",
  "description": "Electron bridge generator for automatic IPC setup (Vite plugin)",
  "keywords": [
    "electron",
    "ipc",
    "bridge",
    "automatic",
    "generator",
    "vite",
    "plugin"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./worker": {
      "types": "./dist/worker.d.ts",
      "import": "./dist/worker.mjs",
      "require": "./dist/worker.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "test": "npm run build && vitest run",
    "pack": "npm run build && screw-up pack --pack-destination ../artifacts"
  },
  "peerDependencies": {
    "async-primitives": ">=0.4.0",
    "chokidar": ">=4.0.3",
    "glob": ">=11.0.3",
    "sublimity-electron-bridge-core": "*",
    "typescript": ">=5.0.0",
    "vite": ">=5.0.0"
  },
  "devDependencies": {
    "@types/node": ">=24.0.10",
    "@vitest/ui": ">=3.2.4",
    "screw-up": ">=0.15.0",
    "typescript": ">=5.8.3",
    "vite": ">=7.0.2",
    "vite-plugin-dts": ">=4.5.4",
    "vitest": ">=3.2.4"
  }
}
